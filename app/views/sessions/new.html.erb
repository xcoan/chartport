<div class="container">
  <div class="page-header">
    <h1>Login</h1>
  </div>
</div>


<%= form_for(:session, url: login_path) do |f| %>
  <div class="container">
    <p>
      <%= f.label :email %><br />
      <%= f.email_field :email, :placeholder => "Email" %>
    </p>

    <p>
      <%= f.label :password %><br />
      <%= f.password_field :password, :placeholder => "Password" %><br />
    </p>

    <%= f.submit "Log In" %>
  </div>
<% end %>
