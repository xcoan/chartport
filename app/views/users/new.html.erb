<div class="container">
  <div class="jumbotron">
    <h1>Sign Up New User</h1>
  </div>
</div>


<div class="container">
  <%= form_for @user do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>

  <%= f.submit "Create" %>
  <% end %>
</div>
